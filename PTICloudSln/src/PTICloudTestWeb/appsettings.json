{
  "Authentication": {
    "AzureAd": {
      "ClientId": "[Your Azure AD App Id]",
      "ClientSecret": "[Your Azure AD App Key]",
      "AADInstance": "https://login.microsoftonline.com/",
      "PostLogoutRedirectUri": "[URL To Redirect To]",
      "Domain": "[Your Tenant Domain ]",
      "TenantId": "[Your Azure Tenant Id]",
      "GraphUrl": "https://graph.windows.net",
      "CallbackPath": "/Home/SignIn"
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}
